{
    "name": "vumanskyi/opengraph",
    "description": "Created a new component for Yii2. The Open Graph component for your website",
    "keywords": ["yii2", "extension", "component", "open-graph", "opengraph", "facebook", "twitter-card", "seo", "Vlad Umanskyi", "VladUmanskyi"],
    "homepage": "https://github.com/vumanskyi/yii2-opengraph",
    "type": "yii2-extension",
    "version": "3.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladyslav Umanskyi",
            "email": "vladumanskyi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0 || 7.4",
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "friendsofphp/php-cs-fixer": "^v3.13.1"
    },
    "autoload": {
        "psr-4": {
            "umanskyi31\\opengraph\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
